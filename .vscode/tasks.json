{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "Debug (linux)",
      "command": "/usr/bin/g++",
      "args": [
        "-g",
        "src/main.cpp",
        "`wx-config",
        "--cxxflags",
        "--libs`",
        "-o",
        "build/debug/gepcommander-linux"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
    }
  ],
  "version": "2.0.0"
}
